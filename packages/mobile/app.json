{
  "expo": {
    "name": "DeckPad Companion",
    "slug": "deckpad",
    "version": "0.0.1",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#030303"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#030303"
      },
      "package": "b3nab.deckpad.mobile"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
