{
  "name": "deckpad",
  "private": true,
  "dependencies": {},
  "devDependencies": {},
  "scripts": {
    "---------- NPM EXEC ----------": "",
    "desktop": "pnpm --filter @deckpad/desktop run",
    "mobile": "pnpm --filter @deckpad/mobile run",
    "cli": "pnpm --filter @deckpad/cli run",
    "sdk": "pnpm --filter @deckpad/sdk run",
    "---------- NPM DEV ----------": "",
    "dev:desktop": "pnpm --filter @deckpad/desktop run dev",
    "dev:mobile": "pnpm --filter @deckpad/mobile run dev",
    "dev:cli": "pnpm --filter @deckpad/cli run dev",
    "dev:sdk": "pnpm --filter @deckpad/sdk run dev",
    "---------- NPM ADD ----------": "",
    "add:desktop": "pnpm --filter @deckpad/desktop add",
    "add:mobile": "pnpm --filter @deckpad/mobile add",
    "add:cli": "pnpm --filter @deckpad/cli add",
    "add:sdk": "pnpm --filter @deckpad/sdk add",
    "---------- NPM ADD-DEV ----------": "",
    "add-dev:desktop": "pnpm --filter @deckpad/desktop add -D",
    "add-dev:mobile": "pnpm --filter @deckpad/mobile add -D",
    "add-dev:cli": "pnpm --filter @deckpad/cli add -D",
    "add-dev:sdk": "pnpm --filter @deckpad/sdk add -D",
    "---------- NPM REMOVE ----------": "",
    "remove:desktop": "pnpm --filter @deckpad/desktop rm",
    "remove:mobile": "pnpm --filter @deckpad/mobile rm",
    "remove:cli": "pnpm --filter @deckpad/cli rm",
    "remove:sdk": "pnpm --filter @deckpad/sdk rm",
    "---------- NPM BUILD ----------": "",
    "build:desktop": "pnpm --filter @deckpad/desktop run build",
    "build:mobile": "pnpm --filter @deckpad/mobile run build",
    "build:cli": "pnpm --filter @deckpad/cli run build",
    "build:sdk": "pnpm --filter @deckpad/sdk run build"
  }
}
